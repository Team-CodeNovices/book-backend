<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.book.dao.BookReport">

    <!-- 게시글 등록 -->
    <insert id="reportInsert" parameterType="BookReportDto">
        insert into bookreport("useridx", "postimage", "bookname", "postcontents", "userlike")
        values(#{useridx}, #{postimage}, #{bookname},#{postcontents},#{userlike})
    </insert>





</mapper>